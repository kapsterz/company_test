
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page


GET           /api/v1/start            controllers.MainController.startOrStop(token: String, host: String)

GET           /api/v1/stop             controllers.MainController.startOrStop(token: String, host: String)

POST          /api/v1/sendData         controllers.MainController.sendData

PUT           /api/v1/processor        controllers.MainController.addProcessor(token: String, host: String)

DELETE        /api/v1/processor        controllers.MainController.deleteProcessor(token: String, host: String)

GET           /api/v1/ping             controllers.MainController.ping
